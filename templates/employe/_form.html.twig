            <option value={{ employe.getId() }}>{{ employe.getUtilisateur().getNomUtilisateur() }} {{ employe.getUtilisateur().getPrenomUtilisateur() }}</option>
        {% endfor %}
        </select>
     </p>
    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}


{% endblock %}

</div>
</div>
</div>
</div>
</section>
</div>

