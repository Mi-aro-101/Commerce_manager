{% extends 'base.html.twig' %}

{% block title %}SectionReponse index{% endblock %}

{% block body %}
    <h1>SectionReponse index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for section_reponse in section_reponses %}
            <tr>
                <td>{{ section_reponse.id }}</td>
                <td>
                    <a href="{{ path('app_section_reponse_show', {'id': section_reponse.id}) }}">show</a>
                    <a href="{{ path('app_section_reponse_edit', {'id': section_reponse.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_section_reponse_new') }}">Create new</a>
{% endblock %}
