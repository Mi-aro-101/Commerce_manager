{% extends 'base.html.twig' %}

{% block title %}TestAptitude index{% endblock %}

{% block body %}
    <h1>TestAptitude index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for test_aptitude in test_aptitudes %}
            <tr>
                <td>{{ test_aptitude.id }}</td>
                <td>
                    <a href="{{ path('app_test_aptitude_show', {'id': test_aptitude.id}) }}">show</a>
                    <a href="{{ path('app_test_aptitude_edit', {'id': test_aptitude.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_test_aptitude_new') }}">Create new</a>
{% endblock %}
